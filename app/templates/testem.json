{
  "before_tests": "broccoli build test-dist",
  "after_tests": "rm -rf test-dist",

  "framework": "qunit",
  "launch_in_ci": [
    "Chrome"
  ],
  "launch_in_dev": [
    "Chrome"
  ],
  "disable_watching": true,
  "src_files": [
    "test-dist/vendor.js",
    "test-dist/app.js",
    "tests/**/*.js"
  ]
}
